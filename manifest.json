{
    "manifest_version": 2,
    "name": "YouTube Ad Skipper",
    "version": "1.0",
    "description": "Speeds up YouTube ads 16x",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  